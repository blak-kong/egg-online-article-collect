{
  "name": "online-article",
  "version": "1.0.0",
  "description": "《网络文章收藏系统》",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "egg": "^2.15.1",
    "egg-scripts": "^2.11.0",
    "egg-sequelize": "^6.0.0",
    "egg-static": "^2.2.0",
    "egg-view-ejs": "^2.0.1",
    "koa-is-json": "^1.0.0",
    "mysql2": "^2.2.5"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "factory-girl": "^5.0.4",
    "sequelize-cli": "^6.2.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development egg-scripts start --daemon --title=egg-server-online-article",
    "stop": "egg-scripts stop --title=egg-server-online-article",
    "dev": "cross-env NODE_ENV=development egg-bin dev",
    "prod": "cross-env NODE_ENV=prod egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "cross-env NODE_ENV=test npx sequelize db:migrate && egg-bin cov",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "blak-kong",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "main": ".autod.conf.js",
  "directories": {
    "test": "test"
  },
  "keywords": []
}
